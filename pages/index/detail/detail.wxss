page {
  padding-bottom: 100rpx;
}

.swiper {
  height: 750rpx;
  width: 100%;
  background-color: #fff;
}

.swiper image {
  height: 100%;
  width: 100%;
}

.detail_inf {
  width: 100%;
  background-color: #fff;
  position: relative;
}

.detail_price {
  font-size: 42rpx;
  color: #f00;
  display: block;
  height: 90rpx;
  line-height: 90rpx;
  text-indent: 0.5rem;
}

.detail_price text {
  font-size: 24rpx;
}

.detail_title {
  display: block;
  font-size: 30rpx;
  line-height: 36rpx;
  padding: 0 20rpx 20rpx 20rpx;
}

.btn_share {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  background-color: #027951;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  z-index: 9;
  opacity: 0.7;
}

.btn_share::before {
  content: "";
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  left: 50%;
  top: 50%;
  margin-left: -16rpx;
  margin-top: -15rpx;
  background: url(data:image/png;base64,) center top no-repeat;
  background-size: 30rpx 30rpx;
}

.hover_bg {
  opacity: 1;
}

.detail_inf2 {
  width: 100%;
  background-color: #fff;
  border-bottom: 2rpx solid #eee;
  margin-bottom: 20rpxm;
  position: relative;
  font-size: 24rpx;
  color: #888;
  overflow: hidden;
  padding-bottom: 20rpx;
  display: flex;
}

.detail_inf2 view {
  width: 250rpx;
  margin: 0 auto;
}

.detail_inf2 view:nth-child(1) {
  padding-left: 20rpx;
}

.detail_inf2 view:nth-child(2) {
  text-align: center;
}

.detail_inf2 view:nth-child(3) {
  text-align: right;
  padding-right: 20rpx;
}

.detail_data {
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
  border-top: 2rpx solid #eee;
  border-bottom: 2rpx solid #eee;
  margin-bottom: 20rpx;
  overflow: hidden;
}

.detail_data view.detail_inf_title {
  height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #eee;
  margin: 0 auto;
}

.detail_data view {
  width: 710rpx;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px dotted #eee;
  font-size: 24rpx;
}

.detail_data_name {
  float: left;
  width: 210rpx;
  color: #888;
}

.detail_data_cont {
  text-align: right;
  width: 500rpx;
  float: left;
}

.detail_data view:last-child {
  border: 0;
}

.detail_content {
  width: 100%;
  background-color: #fff;
  border-top: 2rpx solid #eee;
  border-bottom: 2rpx solid #eee;
  margin-bottom: 20rpx;
  overflow: hidden;
}

.detail_content image {
  width: 100%;
}

.btm_bar {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  z-index: 999;
  border-top: 1rpx solid #c8c8c8;
}

.btn_buy {
  width: 300rpx;
  height: 100rpx;
  display: block;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  background-color: #027951;
  color: #fff;
  font-weight: bold;
  position: absolute;
  right: 0;
  top: 0;
}

.btn_add {
  width: 300rpx;
  height: 100rpx;
  display: block;
  line-height: 100rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #f19149;
  color: #fff;
  font-weight: bold;
  position: absolute;
  right: 300rpx;
  top: 0;
  border-right: 1rpx solid #eee;
}

.btn_disable {
  background-color: #ddd;
}

.btn_cart {
  width: 135rpx;
  height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 9;
}

.btn_cart::before {
  content: "";
  position: absolute;
  top: 50%;
  margin-top: -25rpx;
  left: 50%;
  width: 50rpx;
  height: 50rpx;
  margin-left: -25rpx;
  background: url(data:image/png;base64,) center top no-repeat;
  background-size: contain;
}

.btn_cart text {
  position: absolute;
  top: 10rpx;
  left: 80rpx;
  padding: 0 10rpx;
  height: 30rpx;
  background-color: #f00;
  border-radius: 18rpx;
  text-align: center;
  line-height: 30rpx;
  color: #fff;
  font-size: 20rpx;
}

.pop_bg {
  z-index: 999;
  position: fixed;
  background: rgba(0, 0, 0, 0.7);
  top: 0;
  width: 100%;
  height: 100%;
}


.pop_select_cont {
  width: 100%;
  min-height: 180rpx;
  background-color: #fff;
  position: absolute;
  overflow: visible;
  bottom: 0rpx;
}

.pop_select_title {
  position: relative;
  min-height: 3rem;
}

.pop_select_title::before {
  content: "";
  position: absolute;
  width: 710rpx;
  height: 1rpx;
  background-color: #eee;
  left: 20rpx;
  bottom: -10rpx;
}

.pop_select_title image {
  width: 160rpx;
  height: 160rpx;
  border: 1px solid #eee;
  position: absolute;
  left: 20rpx;
  top: -40rpx;
  border-radius: 6rpx;
}

.pop_detail_title {
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  width: 450rpx;
  margin-left: 210rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 24rpx;
}

.pop_detail_price {
  display: inline-block;
  margin-left: 210rpx;
  color: #f00;
  font-size: 36rpx;
  position: relative;
  top: -20rpx;
}
.pop_detail_price text{ font-size: 24rpx;}

.pop_select_num {
  line-height: 120rpx;
  height: 120rpx;
  position: relative;
}

.pop_buy_num {
  margin-left: 20rpx;
  position: relative;
  font-size: 30rpx;
  top: -12rpx;
}

.pop_buy_num text {
  position: absolute;
  left: 0rem;
  top: -4rpx;
  display: block;
  width: 200rpx;
  font-size: 24rpx;
  color: #888;
}

.num_select {
  position: absolute;
  right:20rpx;
  top: 50%;
  display: flex;
  margin-top: -30rpx;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.num_select view {
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  text-align: center;
  font-size: 36rpx;
  color: #888;
}

.num_select view.btn_disable2 {
  color: #ddd;
}

.num_select input {
  border-left: 1rpx solid #ddd;
  border-right: 1rpx solid #ddd;
  display: inline-block;
  font-size: 30rpx;
  color: #888;
  height:60rpx;
  line-height: 60rpx;
  width: 80rpx;
  text-align: center;
}

.btn_pop_close {
  display: block;
  height: 60rpx;
  width: 60rpx;
  line-height: 60rpx;
  font-size: 60rpx;
  font-weight: lighter;
  color: #888;
  border-radius: 50%;
  position: absolute;
  right: 0rpx;
  top: 8rpx;
  text-align: center;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}

.pop_submit {
  display: block;
  width: 100%;
  height:100rpx;
  line-height: 100rpx;
  background-color: #027951;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
