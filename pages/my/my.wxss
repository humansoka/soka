.my_login {
  width: 100%;
  background-color: #fff;
  height: 260rpx;
  position: relative;
  border-bottom: 2rpx solid #eee;
}

.my_login image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 6rpx solid rgba(255, 255, 255, 0.5);
  position: absolute;
  left: 50%;
  margin-left: -50rpx;
  top: 40rpx;
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.1);
}

.my_login text {
  text-align: center;
  width: 100%;
  display: block;
  position: relative;
  top: 160rpx;
}

.setting_link {
  height: 90rpx;
  background-color: #fff;
  border-top: 2rpx solid #eee;
  border-bottom: 2rpx solid #eee;
  margin-top: 0.5rem;
}

.setting_link view {
  height: 90rpx;
  position: relative;
}

.hover_bg {
  background-color: #f5f5f5;
}

.setting_link view image {
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -20rpx;
  width: 40rpx;
  height: 40rpx;
}

.setting_link text {
  margin-left: 30rpx;
  line-height: 90rpx;
  font-size: 30rpx;
}

.my_order {
  width: 100%;
  background-color: #fff;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  margin-top: 20rpx;
  overflow: hidden;
}

.my_order_title {
  width: 690rpx;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #eee;
  position: relative;
}

.my_order_title text {
  font-size: 30rpx;
}

.my_order_title view {
  display: block;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
  font-size: 24rpx;
  padding-right: 30rpx;
}

.my_order_title view::before {
  content: "";
  position: absolute;
  top: 50%;
  right: -10rpx;
  width: 40rpx;
  height: 40rpx;
  margin-top: -20rpx;
  background: url(data:image/png;base64,) center top no-repeat;
  background-size: 40rpx 40rpx;
}

.my_order_tab {
  width: 100%;
  height: 160rpx;
}

.my_order_tab view {
  width: 33.3%;
  display: inline-block;
  text-align: center;
  font-size: 30rpx;
  position: relative;
}

.my_order_tab view text.order_name {
  position: relative;
  display: block;
  text-align: center;
  top: 100rpx;
  color: #666;
  font-size: 24rpx;
}

.my_order_tab view image{
  position: absolute;
  top: 35rpx;
  left: 50%;
  width: 60rpx;
  height: 60rpx;
  margin-left: -30rpx;
}





.my_order_tab view text.order_num {
  position: absolute;
  top: 20rpx;
  left: 160rpx;
  padding: 0 10rpx;
  height: 36rpx;
  background-color: #f00;
  border-radius: 18rpx;
  text-align: center;
  line-height: 36rpx;
  color: #fff;
  font-size:24rpx;
}
